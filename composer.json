{
    "name": "saintsystems/nova-resource-group-menu",
    "version":"0.1.0",
    "description": "Resource group sub-menus for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "resources"
    ],
    "authors": [
        {
            "name": "Saint Systems"
        }
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "laravel/nova": "~2.0|~3.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SaintSystems\\Nova\\ResourceGroupMenu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SaintSystems\\Nova\\ResourceGroupMenu\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
